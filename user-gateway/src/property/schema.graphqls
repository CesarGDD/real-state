type Property {
    property_id: Int!
    address: String!
    status: String!
    rooms: Int!
    toilets: Int!
    park: Int!
    size: Int!
    description: String!
    features: String!
    agent_id: Int!
    agent: Agent!
}

type Query{
    property(property_id: Int!): Property!
    listProperty:[Property!]!
    listPropertyByAgent(agent_id: Int!): [Property!]
}